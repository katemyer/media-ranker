<h2>List of Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user|  %>
      <tr>
        <td><%= link_to user[:username], user_path(user.id), method: :get  %></td>
        <td><%= user.get_vote_count %></td>
        <td><%= user[:created_at] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Back to Media List", root_path, method: :get, class:"btn btn-primary" %>